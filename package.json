{
  "name": "n8n-nodes-dynamics365-business-central",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Microsoft Dynamics 365 Business Central providing 12 resources and 100+ operations for ERP automation including customers, vendors, items, sales orders, purchase orders, invoices, payments, and financial entries.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-dynamics365-business-central.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-dynamics365-business-central#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-dynamics365-business-central/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "dynamics365",
    "business-central",
    "microsoft",
    "erp",
    "automation",
    "workflow",
    "sales",
    "purchasing",
    "inventory",
    "finance",
    "accounting",
    "oauth2",
    "azure-ad"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "COMMERCIAL_LICENSE.md",
    "LICENSING_FAQ.md",
    "README.md"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/Dynamics365BusinessCentralOAuth2Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Dynamics365BusinessCentral/Dynamics365BusinessCentral.node.js",
      "dist/nodes/Dynamics365BusinessCentral/Dynamics365BusinessCentralTrigger.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "*",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
